<?xml version="1.0" encoding="UTF-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:views="clr-namespace:Food.E.Views"
       x:Class="Food.E.AppShell">

  <TabBar>
    <ShellContent Title="Home" ContentTemplate="{DataTemplate views:HomePage}" />
    <ShellContent Title="Community" ContentTemplate="{DataTemplate views:CommunityFeedPage}" />
    <ShellContent Title="Cart" ContentTemplate="{DataTemplate views:CartPage}" />
    <ShellContent Title="Orders" ContentTemplate="{DataTemplate views:OrderTrackingPage}" />
    <ShellContent Title="Admin Menu" ContentTemplate="{DataTemplate views:AdminMenuPage}" />
    <ShellContent Title="Admin Orders" ContentTemplate="{DataTemplate views:AdminOrdersPage}" />
    <ShellContent Title="Rider Dashboard" ContentTemplate="{DataTemplate views:RiderDashboardPage}" />
  </TabBar>

</Shell>
